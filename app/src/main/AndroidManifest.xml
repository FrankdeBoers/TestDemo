        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NewApplication"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SecondActivity"
            android:exported="true" />

        <activity
            android:name=".weakreference.WeakReferenceActivity"
            android:exported="true" />
        <activity
        android:name=".threadpool.ThreadPoolActivity"
        android:exported="true" />

        <activity
            android:name=".coroutine.CoroutineActivity"
            android:exported="true" />

        <activity
            android:name=".BitmapActivity.BitmapActivity"
            android:exported="true" />

        <activity
            android:name=".http.OkhttpActivity"
            android:exported="true" />
        <activity
            android:name=".handler.BarrierActivity"
            android:exported="true" />
        <activity android:name="com.frank.newapplication.rv.RecyclerViewActivity" />

        <activity
            android:name=".handler.BarrierActivity2"
            android:exported="true" />

        <activity
            android:name="com.example.syncbarrierdemo.SecureActivity"
            android:exported="true" />

        <activity
            android:name=".KeyStoreDemoActivity"
            android:exported="true" />

        <activity
            android:name=".BiometricFragmentHostActivity"
            android:exported="true" />

        <activity android:name=".ThreadStateActivity" android:exported="true" />

        <activity android:name=".ServiceManagerCacheActivity" android:exported="true" />

    </application>

</manifest>